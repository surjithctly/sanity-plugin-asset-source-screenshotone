{
  "name": "sanity-plugin-asset-source-screenshotone",
  "version": "1.0.2",
  "description": "Sanity Plugin to generate Screenshots from URL",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "prepublishOnly": "sanipack build && sanipack verify"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/surjithctly/sanity-plugin-asset-source-screenshotone.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin",
    "url-screenshot"
  ],
  "author": "Surjith S M <mail@surjithctly.in>",
  "license": "MIT",
  "dependencies": {
    "@sanity/ui": "^0.37.22",
    "prop-types": "^15.8.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "sanipack": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "bugs": {
    "url": "https://github.com/surjithctly/sanity-plugin-asset-source-screenshotone/issues"
  },
  "homepage": "https://github.com/surjithctly/sanity-plugin-asset-source-screenshotone#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": false
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier"
    ],
    "ignorePatterns": [
      "dist/**/"
    ]
  }
}
